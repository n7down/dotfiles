
for file in ~/.{aliases,exports,path,functions,docker_functions,env}; do
if [[ -r "$file" ]] && [[ -f "$file" ]]; then
source "$file"
fi
done
unset file

export ZSH=$HOME/.oh-my-zsh

ZSH_THEME="gitster"

plugins=(
	git 
	docker 
	tmux 
	zsh-autosuggestions 
	tig
	httpie
	golang
)

source $ZSH/oh-my-zsh.sh

# fzf
[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh

# turn off C-s
stty -ixon

# cloud sdk - linux
if [ -f '/home/josh/Downloads/google-cloud-sdk/path.zsh.inc' ]; then . '/home/josh/Downloads/google-cloud-sdk/path.zsh.inc'; fi
if [ -f '/home/josh/Downloads/google-cloud-sdk/completion.zsh.inc' ]; then . '/home/josh/Downloads/google-cloud-sdk/completion.zsh.inc'; fi

# cloud sdk - home
if [ -f '$(brew --prefix)/Caskroom/google-cloud-sdk/latest/google-cloud-sdk/path.zsh.inc' ]; then . '$(brew --prefix)/Caskroom/google-cloud-sdk/latest/google-cloud-sdk/path.zsh.inc'; fi
if [ -f '$(brew --prefix)/Caskroom/google-cloud-sdk/latest/google-cloud-sdk/completion.zsh.inc' ]; then . '$(brew --prefix)/Caskroom/google-cloud-sdk/latest/google-cloud-sdk/completion.zsh.inc'; fi

# autojump - home
[ -f /usr/local/etc/profile.d/autojump.sh ] && . /usr/local/etc/profile.d/autojump.sh

# autojump - linux
[[ -s /home/josh/.autojump/etc/profile.d/autojump.sh ]] && source /home/josh/.autojump/etc/profile.d/autojump.sh

autoload -U compinit && compinit -u

# linux brew for linux
case $(uname) in
	'Linux') eval $(/home/linuxbrew/.linuxbrew/bin/brew shellenv) ;;
esac

