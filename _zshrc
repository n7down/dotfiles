
for file in ~/.{aliases,exports,path,functions,docker_functions,env,divvy_aliases}; do
if [[ -r "$file" ]] && [[ -f "$file" ]]; then
    source "$file"
fi
done
unset file

#export ZSH=$HOME/.oh-my-zsh

ZSH_THEME="gitster"

plugins=(
	git 
	docker 
   	docker-compose
	tmux 
	zsh-autosuggestions 
	tig
	httpie
	golang
)

source $ZSH/oh-my-zsh.sh

#autoload -Uz compinit
#compinit
#zstyle ':completion:*' menu select

# fzf
[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh

# turn off C-s
# stty -ixon

# autojump
[ -f /usr/local/etc/profile.d/autojump.sh ] && . /usr/local/etc/profile.d/autojump.sh

# asdf
[ -f $(brew --prefix asdf)/asdf.sh ] && . $(brew --prefix asdf)/asdf.sh
[ -f $(brew --prefix asdf)/etc/bash_completion.d/asdf.bash ] && . $(brew --prefix asdf)/etc/bash_completion.d/asdf.bash
