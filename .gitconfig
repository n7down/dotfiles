[alias]
	ci = commit
	br = branch
	l = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short
	cf = config

    	a = add
    	ap = add -p
    	c = commit --verbose
    	ca = commit -a --verbose
    	cm = commit -m
    	cam = commit -a -m
    	m = commit --amend --verbose

    	d = diff
    	ds = diff --stat
    	dc = diff --cached

    	s = status -s
    	co = checkout
    	cob = checkout -b
